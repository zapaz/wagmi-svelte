{
  "name": "@zapaz/wagmi-svelte",
  "version": "0.2.2",
  "license": "MIT",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "package": "svelte-kit sync && svelte-package -o 'dist/lib' && publint",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write ."
  },
  "files": [
    "package.json",
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "exports": {
    ".": {
      "types": "./dist/lib/index.d.ts",
      "svelte": "./dist/lib/index.js"
    },
    "./chains": {
      "types": "./dist/lib/chains/index.d.ts",
      "svelte": "./dist/lib/chains/index.js"
    },
    "./connectors": {
      "types": "./dist/lib/connectors/index.d.ts",
      "svelte": "./dist/lib/connectors/index.js"
    }
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.2.5",
    "@sveltejs/kit": "^2.6.1",
    "@sveltejs/package": "^2.3.5",
    "@sveltejs/vite-plugin-svelte": "4.0.0-next.7",
    "@tailwindcss/typography": "^0.5.15",
    "@types/eslint": "^9.6.1",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "2.44.1",
    "mdsvex": "^0.11.2",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.7",
    "publint": "^0.2.11",
    "svelte-check": "^4.0.4",
    "tailwindcss": "^3.4.13",
    "tslib": "^2.7.0",
    "typescript": "^5.6.2",
    "vite": "^5.4.8",
    "vitest": "^2.1.2"
  },
  "type": "module",
  "dependencies": {
    "@tanstack/svelte-query": "^5.59.0",
    "@wagmi/connectors": "^5.1.14",
    "@wagmi/core": "^2.13.8",
    "svelte": "5.0.0-next.260",
    "viem": "^2.21.17"
  }
}